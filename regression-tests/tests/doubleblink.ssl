toggle(led : Ref (Sched Int)) =
  led <- 1 - led

slow(led : Ref (Sched Int)) =
  let e1 = Occur : Sched Event
  loop
    toggle led
    after 30 ms , e1 <- Occur
    wait e1

fast(led : Ref (Sched Int)) =
  let e2 = Occur : Sched Event
  loop
    toggle led
    after 20 ms , e2 <- Occur
    wait e2

main(led : Ref (Sched Int)) =
  par slow led
      fast led
