type List
  Cons Int List
  Nil

puts cout s =
  match s
    Cons c ss =
      after 1, cout <- c
      wait cout
      after 1, cout <- 32
      wait cout
      puts cout ss
    Nil = ()

main cin cout =
 let lst = [49,50,51] // will uncomment this when list syntax works end-to-end
 puts cout lst
